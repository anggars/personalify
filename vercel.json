{
  "framework": "nextjs",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/stats",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/clear",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/sync",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/export",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/report/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/login",
      "destination": "/api/index.py"
    },
    {
      "source": "/callback",
      "destination": "/api/index.py"
    },
    {
      "source": "/logout",
      "destination": "/api/index.py"
    },
    {
      "source": "/auth/refresh",
      "destination": "/api/index.py"
    },
    {
      "source": "/analyze-lyrics",
      "destination": "/api/index.py"
    },
    {
      "source": "/request-access",
      "destination": "/api/index.py"
    },
    {
      "source": "/sync/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/analyze-sentiment-background",
      "destination": "/api/index.py"
    },
    {
      "source": "/resume",
      "destination": "/resume.html"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/redoc",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 25
    }
  }
}