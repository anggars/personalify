{
  "builds": [
    {
      "source": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "source": "/docs",
      "destination": "https://[subdomain].mintlify.dev/docs"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://[subdomain].mintlify.dev/docs/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "api/index.py"
    }
  ]
}