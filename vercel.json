{
  "framework": "nextjs",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin-api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/login",
      "destination": "/api/index.py"
    },
    {
      "source": "/callback",
      "destination": "/api/index.py"
    },
    {
      "source": "/logout",
      "destination": "/api/index.py"
    },
    {
      "source": "/auth/refresh",
      "destination": "/api/index.py"
    },
    {
      "source": "/analyze-lyrics",
      "destination": "/api/index.py"
    },
    {
      "source": "/request-access",
      "destination": "/api/index.py"
    },
    {
      "source": "/sync/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/analyze-emotions-background",
      "destination": "/api/index.py"
    },
    {
      "source": "/resume",
      "destination": "/resume.html"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/redoc",
      "destination": "/api/index.py"
    }
  ]
}