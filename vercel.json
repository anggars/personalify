{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "https://personalify.mintlify.app/"
    },
    {
      "src": "/docs/(.*)",
      "dest": "https://personalify.mintlify.app/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "https://personalify.mintlify.app/_next/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "https://personalify.mintlify.app/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}